                                <table class="table table-condensed table-bordered text-center">
                                    <thead>
                                    <tr>
                                        <th class="text-center">原料</th>
                                        <th class="text-center">单耗（kg/KL）</th>
                                        <th class="text-center">单价（元/kg）</th>
                                        <th class="text-center">费用（元/KL）</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td>草甘膦94%以上原粉（95%）</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>异丙胺</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>助剂</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>液氨</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!--加工成分表结束-->
                        <!--理化指标表开始-->
                        <div class="form-group ">
                            <label class="col-xs-1 control-label">理化指标</label>
                            <div class="col-xs-8">
                                <table class="table table-condensed table-bordered text-center">
                                    <thead>
                                    <tr>
                                        <th class="col-xs-4 text-center">指标</th>
                                        <th class="col-xs-4 text-center">值</th>
                                        <th class="col-xs-4 text-center">备注</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td>外观</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>草甘膦含量（g/L）</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>对应盐含量（g/L）</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>PH值</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>稀释稳定性（20倍）</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>甲醛含量（g/Kg）</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>比重（20±3℃）</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>低温稳定性</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>热稳定性</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    <tr>
                                        <td>水不溶物</td>
                                        <td><input class="form-control" type="text" /></td>
                                        <td><input class="form-control" type="text" /></td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!--理化指标表结束-->
                        <div class="form-group">
                            <label class="col-xs-1 control-label">备注</label>
                            <div class="col-xs-8">
