            <!--表格开始-->
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        产品档案
                    </h3>
                </div>
                <div class="panel-body clearfix">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-xs-1 control-label">客户名称</label>
                            <div class="col-xs-2">
                                <input class="form-control" type="text" />
                            </div>
                            <label class="col-xs-1 control-label">加工厂</label>
                            <div class="col-xs-2">
                                <input class="form-control" type="text"  />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-xs-1 control-label">品类</label>
                            <div class="col-xs-2">
                                <input class="form-control" type="text" />
                            </div>
                            <label class="col-xs-1 control-label">品名</label>
                            <div class="col-xs-2">
                                <input class="form-control" type="text" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-xs-1 control-label">含量（%）</label>
                            <div class="col-xs-2">
                                <input class="form-control" type="text"/>
                            </div>
                            <label class="col-xs-1 control-label">盐类</label>
                            <div class="col-xs-7">
                                <label class="radio-inline"><input type="radio">无</label>
                                <label class="radio-inline"><input type="radio">异丙胺盐</label>
                                <label class="radio-inline"><input type="radio">铵盐</label>
                                <label class="radio-inline"><input type="radio">钾盐</label>
                                <label class="radio-inline"><input type="radio">二钾铵盐</label>
                                <label class="radio-inline"><input type="radio">其他</label>
                                <label class="radio-inline"><input type="text"></label>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label class="col-xs-1 control-label">执行标准</label>
                            <div class="col-xs-2">
                                <select class="form-control">
                                    <option value="" selected>国际标准</option>
                                    <option value="">国家 标准</option>
                                    <option value="">行业标准</option>
                                    <option value="">福华通达企业标准</option>
                                    <option value="">其他</option>
                                </select>
                            </div>
