<div class="clearfix content shadow-out">
    <!--副导航-->
    <div class="nav-deputy shadow-out clearfix">
        #deputyNav(1)
        #breadcrumb()
    </div>
    <!--表格容器-->
    <div class="dashboard-wrapper">
        <div class="row">
            <div class="col-xs-12">
                <!--客户基本信息-->
                <div class="panel panel-default panel-edit view-HD-lg">
                    <div class="panel-heading">
                        <button id="editCustomerBasic" data-form-btn="edit" class="btn btn-xs btn-primary panel-heading-btn pull-right" type="button" lang="zh">
                            编辑
                        </button>
                        <div class="but-group btn-group btn-group-xs panel-heading-btn pull-right hidden">
                            <button id="cancelCustomerBasic" data-form-btn="cancel" class="btn" lang="zh">取消</button>
                            <button id="saveCustomerBasic" data-form-btn="save" class="btn btn-primary" lang="zh">保存</button>
                        </div>
                        <h3 id="sTitle" class="panel-title" lang="zh">
                            基本信息
                        </h3>
                    </div>
                    <div class="panel-body clearfix">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    客户编号
                                </label>
                                <div class="col-xs-2">
                                    <input name="customerId" class="form-control noEdit" disabled type="text" value="" />
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    客户全称<sup class="not-null editView hidden">*</sup>
                                </label>
                                <div class="col-xs-2">
                                    <input name="fullName" class="form-control notNull" type="text" value="" />
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    客户简称
                                </label>
                                <div class="col-xs-2">
                                    <input id="shortName" class="form-control" type="text" value="" />
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    地区分类<sup class="not-null editView hidden">*</sup>
                                </label>
                                <div class="col-xs-2">
                                    <select id="areaId" class="form-control notNull">
                                        <option lang="zh" value="-1" selected>——请选择地区——</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    国家<sup class="not-null editView hidden">*</sup>
                                </label>
                                <div class="col-xs-2">
                                    <select id="countryId" class="form-control notNull">
                                        <option lang="zh" value="-1" selected>——请优先选择地区——</option>
                                    </select>
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    中信保编码
                                </label>
                                <div class="col-xs-2">
                                    <input id="zhongxinbaoNumber" class="form-control" type="text" value="" />
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    信用评级
                                </label>
                                <div class="col-xs-2">
                                    <div class="input-group">
                                        <select id="zhongxinbaoLevel" class="form-control">
                                            <option value="1" selected>A</option>
                                            <option value="2">AA</option>
                                            <option value="3">AAA</option>
                                            <option value="4">AAAA</option>
                                            <option value="-1" lang="zh">无</option>
                                        </select>
                                        <span class="input-group-addon"><a href="http://www.sinosure.com.cn/sinosure/index.html" target="_blank">快速查询</a></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    企业性质<sup class="not-null editView hidden">*</sup>
                                </label>
                                <div class="col-xs-6">
                                    <label class="checkbox-inline">
                                        <input id="showFactory" data-target="#factory" name="enterpriseNature" class="else notNull" type="checkbox" value="1" />
                                        <span lang="zh">工厂</span>
                                    </label>
                                    <label class="checkbox-inline">
                                        <input name="enterpriseNature" class="notNull" type="checkbox" value="2" />
                                        <span lang="zh">经销商</span>
                                    </label>
                                    <label class="checkbox-inline">
                                        <input id="showMajorCompetitorsGroup" data-target="#majorCompetitorsGroup" name="enterpriseNature" class="else notNull" type="checkbox" value="3" />
                                        <span lang="zh">分销商</span>
                                    </label>
                                    <label class="checkbox-inline">
                                        <input name="enterpriseNature" class="notNull" type="checkbox" value="4" />
                                        <span lang="zh">终端客户</span>
                                    </label>
                                    <label class="checkbox-inline">
                                        <input id="showOtherEnterpriceNature" data-target="#otherEnterpriceNature" name="enterpriseNature" class="else notNull" type="checkbox" value="5"/>
                                        <span lang="zh">其他</span>
                                    </label>
                                    <span class="form-inline">
                                        <input id="otherEnterpriceNature" class="form-inline form-control hidden" style="position:absolute;left:355px;" type="text" value="" placeholder="请输入企业性质" lang="zh"/>
                                    </span>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    注册资金
                                </label>
                                <div class="col-xs-2">
                                    <div class="input-group">
                                        <input id="registeredFunds" class="form-control" type="text" value="" />
                                        <span class="input-group-addon">$</span>
                                    </div>
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    企业电话
                                </label>
                                <div class="col-xs-2">
                                    <input id="enterprisePhone" class="form-control" type="tel" value="" />
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    企业邮箱
                                </label>
                                <div class="col-xs-2">
                                    <input id="enterpriseEmail" class="form-control" type="email" value="" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    注册地址
                                </label>
                                <div class="col-xs-8">
                                    <textarea id="registeredAddress" class="form-control" rows="2" ></textarea>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    经营范围
                                </label>
                                <div class="col-xs-8">
                                    <textarea id="managementScope" class="form-control" rows="2" ></textarea>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    是否有中国分公司
                                </label>
                                <div class="col-xs-2">
                                    <select id="hasChiCompany" class="form-control" >
                                        <option value="1" lang="zh" selected>有</option>
                                        <option value="0" lang="zh">无</option>
                                    </select>
                                </div>
                                <label class="control-label col-xs-1 text-left" lang="zh">
                                    是否有中方采购
                                </label>
                                <div class="col-xs-2">
                                    <select id="hasChiPurchase" class="form-control" >
                                        <option value="1" lang="zh" selected>有</option>
                                        <option value="0" lang="zh">无</option>
                                    </select>
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    客户类型
                                </label>
                                <div class="col-xs-2">
                                    <select id="customerLevel" class="form-control" >
                                        <option value="-1" selected >——选择客户类型——</option>
                                        <option value="1" lang="zh">战略客户</option>
                                        <option value="2" lang="zh">大客户</option>
                                        <option value="3" lang="zh">重要客户</option>
                                        <option value="4" lang="zh">一般客户</option>
                                        <option value="5" lang="zh">风险客户</option>
                                    </select>
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    客户状态
                                </label>
                                <div class="col-xs-2">
                                    <select name="customerType" class="form-control" >
                                        <option value="1" lang="zh" >合作</option>
                                        <option id="showOpportunity" data-target="#opportunity" value="2" lang="zh">潜在</option>
                                        <option value="3" lang="zh" >流失</option>
                                    </select>
                                    <button id="showReasons" data-toggle="modal" data-target="#reasons" type="button" style="display: none;"></button>
                                </div>
                            </div>
##                            modal
                            <div class="modal fade" id="reasons" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
