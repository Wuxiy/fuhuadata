<div class="clearfix content shadow-out">
    <!--副导航-->
    <div class="nav-deputy shadow-out clearfix">
        #deputyNav(6)
        #breadcrumb()
    </div>
    <!--表格容器-->
    <div class="dashboard-wrapper">
        <div class="row">
            <!--导航-->
            #orderRequestNav(2)
            <div class="col-xs-10">
                <!--订舱出运要求-->
                <div class="panel panel-default view-HD-md">
                    <div class="panel-heading">
                        <div class="input-group panel-heading-btn pull-right col-xs-4">
                            <span class="input-group-addon" lang="zh">产品种类</span>
                            <select id="name" class="form-control"></select>
                        </div>
                        <h3 id="sTitle" class="panel-title" lang="zh">
                            订舱出运要求
                        </h3>
                    </div>
                    <div id="billPanel" class="panel-body panel-edit">
                        <div class="form-horizontal">
                            <div class="form-group">
                                <label class="control-label col-xs-2" lang="zh">对船运公司要求</label>
                                <div class="col-xs-10">
                                    <textarea id="shippingAgentRequire" disabled cols="3" class="form-control" placeholder="指定的船运公司，或者禁用的船运公司"  lang="zh"></textarea>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="" class="control-label col-xs-2" lang="zh">装箱要求</label>
                                <div class="col-xs-10">
                                    <!--表格-->
                                    <table class="table table-condensed table-bordered table-striped text-right">
                                        <tbody>
                                        <tr>
                                            <td lang="zh">打托（缠膜）</td>
                                            <td class="text-left">
                                                <div class="col-xs-12">
                                                    <label class="checkbox-inline"><input name="tray" type="checkbox" disabled value="1"/>免熏蒸木质托盘</label>
                                                    <label class="checkbox-inline"><input name="tray" type="checkbox" disabled value="2"/>熏蒸托盘</label>
                                                    <label class="checkbox-inline"><input name="tray" type="checkbox" disabled value="3"/>塑料托盘</label>
                                                    <label class="checkbox-inline"><input name="tray" type="checkbox" disabled value="4"/>订制托盘</label>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td lang="zh">贴唛</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="postLabel" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">垫板</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="basePlate" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">纸板</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="paperBoard" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">气囊</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="gasbag" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">拉网</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="dragNet" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">紧固带</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="fastenBelt" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">抄条码</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="barcCode" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">美式门封</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="americanDoorseal" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">护条</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="bead" type="checkbox" disabled value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">护角</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="cornerProtection" disabled type="checkbox" value="1"/></label>
                                                </div>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td lang="zh">是否需要监装</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input name="inspectionOfLoading" disabled type="checkbox" value="1"/>商检</label>
                                                </div>
                                                <div class="checkbox col-xs-3">
                                                    <label><input name="inspectionOfLoading" disabled type="checkbox" value="2"/>法检</label>
                                                </div>
                                                <div class="col-xs-3">
                                                    <input id="inspectionInstitution" type="text" disabled class="form-control" placeholder="输入机构名称"/>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td lang="zh">吨桶是否铁丝固定</td>
                                            <td class="text-left">
                                                <div class="checkbox col-xs-3">
                                                    <label><input id="wireFixed" type="checkbox" disabled value="1"/></label>
                                                </div>
                                            </td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-xs-2" lang="zh">出运货物</label>
                                    <div class="radio col-xs-1">
                                        <label ><input type="radio" name="goodsType" disabled value="0" /><span lang="zh">普货</span</label>
                                    </div>
                                    <div class="radio col-xs-2">
                                        <label ><input type="radio" name="goodsType" disabled value="1" /><span lang="zh">危险品</span></label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-xs-2"><span lang="zh">免箱期</span>（<span lang="zh">天</span>）</label>
                                    <div class="col-xs-2">
                                        <input id="mianxiangqi" class="form-control" type="text" disabled value="" />
                                    </div>
                                    <label class="control-label col-xs-2"><span lang="zh">免堆期</span>（<span lang="zh">天</span>）</label>
                                    <div class="col-xs-2">
                                        <input id="miantuiqi" class="form-control" type="text" disabled value="" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<input id="customerId" type="hidden" value='$!{customerId}'>
<input type="hidden" id="customerType" value="$!{customerType}" />
<input type="hidden" id="fullName" value="$!{fullName}" />

## 产品种类
<script id="nameC" type="text/html">
    <%if (data.length>0) {%>
    <%for (var i=0;i<data.length;i++){%>
    <option value="<%=data[i].wareId%>"><%=data[i].name%></option>
    <%}%>
    <%}%>
</script>

<script type="text/javascript" language="javascript" src="${request.contextPath}/lib/js/commons/common.js"></script>
<script type="text/javascript" language="javascript" src="${request.contextPath}/lib/js/commons/commons.js"></script>
<script type="text/javascript" language="javascript" src="${request.contextPath}/lib/js/commons/ajax.js"></script>
<script type="text/javascript" language="javascript" src="${request.contextPath}/lib/js/customer/common.js"></script>
<script type="text/javascript" language="javascript" src="${request.contextPath}/lib/js/customer/customerOrderRequireAjax.js"></script>
<script type="text/javascript" language="javascript" src="${request.contextPath}/lib/js/customer/customerTransportRequireInteraction.js"></script>


