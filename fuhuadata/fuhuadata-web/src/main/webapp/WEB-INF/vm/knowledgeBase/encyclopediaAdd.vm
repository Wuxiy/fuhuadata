<div class="nav-deputy shadow-out clearfix">
    <ul class="breadcrumb pull-right">
        <li><span class="glyphicon glyphicon-map-marker"></span></li>
    </ul>
</div>
<div class="dashboard-wrapper">
    <div class="row">
        <div class="col-xs-12">
            <!--表格开始-->
            <div class="panel panel-default view-HD-lg">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        新增/编辑百科
                    </h3>
                </div>
                <div class="panel-body clearfix">
                    <form class="form-horizontal">
                        <div class="form-group ">
                            <label for="fullName" class="col-xs-1 control-label">企业全称<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-2">
                                <input id="fullName" name="fullName" class="form-control" disabled type="text"/>
                                <div class="col-xs-12">
                                    <ul id="cName" class="pull-right dropdown-menu" style="width: 100%"></ul>
                                </div>
                            </div>
                            <label for="shortName" class="col-xs-1 control-label">企业简称<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-2">
                                <input id="shortName" name="shortName" class="form-control" disabled type="text"/>
                            </div>
                            <label for="companyType" class="col-xs-1 control-label">企业类别<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-2">
                                <select id="companyType" name="companyType" disabled class="form-control">
                                    <option value="0">客户</option>
                                    <option value="1">竟对</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="customerArea" class="col-xs-1 control-label">地区分类<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-2">
                                <select id="customerArea" name="customerArea" disabled class="form-control">
                                    <option value="">亚洲</option>
                                    <option value="">欧洲</option>
                                </select>
                            </div>
                            <label for="country" class="col-xs-1 control-label">国家<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-2">
                                <select id="country" name="country" disabled class="form-control">
                                    <option value="">美国</option>
                                    <option value="">中国</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-xs-1 control-label">企业性质</label>
                            <div class="col-xs-6">
                                <label class="checkbox-inline">
                                    <input disabled name="enterpriseNature" type="checkbox" value="1">
                                    <span lang="zh">工厂</span>
                                </label>
                                <label class="checkbox-inline">
                                    <input disabled name="enterpriseNature" disabled type="checkbox" value="2">
                                    <span lang="zh">经销商</span>
                                </label>
                                <label class="checkbox-inline">
                                    <input disabled name="enterpriseNature" type="checkbox" value="3">
                                    <span lang="zh">分销商</span>
                                </label>
                                <label class="checkbox-inline">
                                    <input disabled name="enterpriseNature" disabled type="checkbox" value="4">
                                    <span lang="zh">终端客户</span>
                                </label>
                                <label class="checkbox-inline">
                                    <input disabled data-target="#otherEnterpriceNature" name="enterpriseNature" class="else" type="checkbox" value="5">
                                    <span lang="zh">其他</span>
                                </label>
                                <span class="form-inline">
                                    <input disabled="disabled" id="otherEnterpriceNature" class="form-inline form-control hidden" style="position:absolute;left:355px;" type="text" value="" lang="zh">
                                </span>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="registeredFund" class="col-xs-1 control-label">注册资金<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-2">
                                <input id="registeredFund" name="registeredFund" disabled type="text" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="registeredAddr" class="col-xs-1 control-label">注册地址<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-8">
                                <textarea id="registeredAddr" name="registeredAddr" disabled class="form-control" rows="2"></textarea>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label class="col-xs-1 control-label">经营范围<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-8">
                                <textarea disabled class="form-control" rows="2"></textarea>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="managementScope" class="col-xs-1 control-label">企业简介<sup data-view="editView" class="not-null hidden">*</sup></label>
                            <div class="col-xs-8">
                                <textarea id="managementScope" name="managementScope" class="form-control" rows="4"></textarea>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="developHis" class="col-xs-1 control-label">发展历程</label>
                            <div class="col-xs-8">
                                <textarea id="developHis" name="developHis" class="form-control" rows="4"></textarea>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="sellNetwork" class="col-xs-1 control-label">销售网络</label>
                            <div class="col-xs-8">
                                <textarea id="sellNetwork" name="sellNetwork" class="form-control" rows="4"></textarea>
                            </div>
                        </div>
                        <fieldset id="cum"></fieldset>
                        <div class="form-group">
                            <div class="col-xs-1 col-xs-offset-1">
                                <a href="" class="btn-link" data-toggle="modal" data-target="#addField">
                                    <span class="glyphicon glyphicon-plus"></span>添加字段
                                </a>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-xs-2 col-xs-offset-4">
                                <button id="upBtn" class="btn btn-block btn-primary" type="button">完成</button>
                            </div>
                            <div class="col-xs-2">
                                <button id="resetBtn" class="btn btn-block btn-default" type="button">重置</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


<!-- 模态框（Modal） -->
<div class="modal fade" id="addField" tabindex="-1" data-backdrop="static" aria-hidden="true" data-keyboard="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
            </div>
            <div class="modal-body ">
                <form class="form-horizontal" role="form">
                    <div class="form-group">
                        <label class="col-xs-2 control-label">字段名称</label>
                        <div class="col-xs-5">
                            <input class="form-control" type="text"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-xs-2 control-label">描述内容</label>
                        <div class="col-xs-8">
                            <textarea class="form-control" rows="6"></textarea>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-xs-3 col-xs-offset-3">
                        <button type="button" class="btn btn-primary btn-block" id="submit">
                            完成
                        </button>
                    </div>
                    <div class="col-xs-3">
                        <button type="button" class="btn btn-default btn-block" data-dismiss="modal">
                            取消
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

