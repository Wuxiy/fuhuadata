                var data = ids;

                jQuery.ajax({
                    url:url,
                    type:'POST',
                    dataType:"json",
                    contentType:"application/json",
                    data:JSON.stringify(data),
                    success:function(){
                        alert("批量删除成功");
                        location.reload();
                    }
                })
            }
        }else{
            alert("还未选择需要删除的关联！");
        }
    })
    
