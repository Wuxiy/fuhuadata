                                    </div>
                                    <label class="control-label col-xs-1" lang="zh">
                                        生产线
                                    </label>
                                    <div class="col-xs-5">
                                        <textarea id="productLine" class="form-control" rows="2" ></textarea>
                                    </div>
                                </div>
                                <div name="customerMakeProduct" class="form-group" style="position: relative;"><label class="col-xs-1 control-label" style="position: relative">生产产品1</label><div class="col-xs-2"><input name="productName" class="form-control" type="text" value=""></div><label class="col-xs-1  control-label">产能</label><div class="col-xs-2"><input name="production" class="form-control" type="text" value=""></div></div>
                            ##                            这里添加客户生产产品
                                <div class="form-group">
                                    <div class="col-xs-2 col-xs-offset-1">
                                        <button data-form-btn='add' id='addPro' class="btn-link editView" type="button">添加产品</button>
                                    </div>
                                </div>
                            </fieldset>
                            <fieldset id="majorCompetitorsGroup" class="hidden">
                                <div class="form-group">
                                    <label class="control-label col-xs-1" lang="zh">
                                        分销主要竞争对手
                                    </label>
                                    <div class="col-xs-8">
                                        <textarea id="majorCompetitors" class="form-control" rows="4" ></textarea>
                                    </div>
                                </div>
                            </fieldset>
                            <div class="form-group">
                                <label class="control-label col-xs-1" lang="zh">
                                    备注
                                </label>
                                <div class="col-xs-8">
                                    <textarea id="remark" class="form-control" rows="4" ></textarea>
                                </div>
                            </div>
                            <fieldset id="opportunity" class="hidden">
                                <div class="form-group">
                                    <label class="control-label col-xs-1" lang="zh">
                                        机会来源<sup class="not-null editView hidden">*</sup>
                                    </label>
                                    <div class="col-xs-2">
                                        <select id="opportunitySource" class="form-control else notNull">
                                            <option value="-1" selected lang="zh">——请选择机会来源——</option>
                                            <option value="1">展会</option>
                                            <option value="2">门户网站</option>
                                            <option value="3">农药店考察</option>
                                            <option value="4">客户介绍</option>
                                            <option value="5">主动联系</option>
                                            <option id="showOtherOpportunity" data-target="#otherOpportunity" class="else" value="6">其他</option>
                                        </select>
                                    </div>
                                    <div class="col-xs-2">
                                        <input id="otherOpportunity" class="form-control hidden" type="text" placeholder="输入机会来源" lang="zh"/>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-xs-1" lang="zh">
                                        机会描述<sup class="not-null editView hidden">*</sup>
                                    </label>
                                    <div class="col-xs-8">
                                        <textarea id="opportunityDescrible" class="form-control notNull" rows="4" placeholder="描述客户发展机会" lang="zh"></textarea>
                                    </div>
                                </div>
                            </fieldset>
                            <div class="form-group editHide">
                                <label class="control-label col-xs-1" lang="zh">
                                    最后编辑人
                                </label>
                                <div class="col-xs-2">
                                    <input id="lastmodifyUserName" class="form-control noEdit" disabled value="">
                                </div>
                                <label class="control-label col-xs-1" lang="zh">
                                    编辑时间
                                </label>
                                <div class="col-xs-2">
                                    <input id="modifyTime"class="form-control noEdit" disabled value="">
                                </div>
                            </div>
                        </form>
                    </div>
